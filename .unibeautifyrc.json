{
  "JSON": {
    "beautifiers": [
      "Prettier",
      "Pretty Diff",
      "JS-Beautify"
    ],
    "align_assignments": true,
    "break_chained_methods": true
  },
  "JSX": {
    "beautifiers": [
      "Prettier",
      "Pretty Diff",
      "JS-Beautify"
    ],
    "break_chained_methods": true
  },
  "JavaScript": {
    "beautifiers": [
      "Prettier",
      "Pretty Diff",
      "JS-Beautify",
      "ESLint"
    ],
    "break_chained_methods": true
  },
  "SVG": {
    "beautifiers": [
      "Pretty Diff"
    ],
    "align_assignments": true,
    "break_chained_methods": true,
    "indent_comments": true
  },
  "HTML": {
    "beautifiers": [
      "Pretty Diff",
      "JS-Beautify"
    ],
    "break_chained_methods": true,
    "indent_size": 4,
    "newline_before_tags": [],
    "wrap_attributes": "auto"
  },
  "Markdown": {
    "beautifiers": [
      "Prettier"
    ]
  },
  "CSS": {
    "beautifiers": [
      "Prettier",
      "Pretty Diff",
      "JS-Beautify"
    ]
  },
  "XML": {
    "beautifiers": [
      "Pretty Diff",
      "JS-Beautify"
    ]
  }
}